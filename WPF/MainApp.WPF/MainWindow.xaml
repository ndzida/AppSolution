<Window x:Class="MainApp.WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MainApp.WPF"
        xmlns:uc="clr-namespace:MainApp.WPF.UserControls"
        mc:Ignorable="d"
        Title="MainWindow">
    <Grid>
        <TabControl>
            <TabItem Header="Generate models">
                <uc:GenerateModelsUserControl Foreground="Black"></uc:GenerateModelsUserControl>
            </TabItem>
            <TabItem Header="Administrate user rights">
                <uc:AdministrateUserRightsUserControl Foreground="Black"></uc:AdministrateUserRightsUserControl>
            </TabItem>
            <TabItem Header="Data encryption">
                <uc:DataEncryptionUserControl Foreground="Black"></uc:DataEncryptionUserControl>
            </TabItem>
            <TabItem Header="Chat">
                <uc:ChatUserControl Foreground="Black"></uc:ChatUserControl>
            </TabItem>
        </TabControl>
    </Grid>
</Window>
